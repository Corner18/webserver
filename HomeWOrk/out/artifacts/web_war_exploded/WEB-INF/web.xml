<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>profile</servlet-name>
        <servlet-class>ProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>friends</servlet-name>
        <servlet-class>FriendsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>friends</servlet-name>
        <url-pattern>/friends</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>im</servlet-name>
        <servlet-class>IncomingMessagesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>im</servlet-name>
        <url-pattern>/im</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>om</servlet-name>
        <servlet-class>OutgoingMessagesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>om</servlet-name>
        <url-pattern>/om</url-pattern>
    </servlet-mapping>




    <servlet>
        <servlet-name>dialogue</servlet-name>
        <servlet-class>DialogueServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dialogue</servlet-name>
        <url-pattern>/messages</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>




</web-app>